#include <iostream>
using namespace std;
int main()
{
    const int N = 10;
    int arr[N];
    int d = 0;
 
    for (int i = 0; i < N; i++)
        cin >> arr[i];
 
    for (int i = 0; i < N; i++)
        if (arr[i] % 2 == 1)
            d = arr[i];
 
    if (d != 0)
        for (int i = 0; i < N; i++)
            if (arr[i] % 2 == 1)
                arr[i] += d;
 
    for (int i = 0; i < N; i++)
        cout << arr[i] << " ";
}
