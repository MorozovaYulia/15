#include <iostream>
using namespace std;
int main()
{
    const int N = 10;
    int A[N], a=0;
    for (int i = 0; i < N; i++)
        cin >> A[i];
    for (int i = 0; i < N; i++)
        if (A[i] % 2 == 1)
            a = A[i];
    if (a != 0)
        for (int i = 0; i < N; i++)
            if (A[i] % 2 == 1)
                A[i] += a;
    for (int i = 0; i < N; i++)
        cout << A[i] << " ";
}
