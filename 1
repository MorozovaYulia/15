#include <iostream>
using namespace std;
int main()
{
    const int N = 3;
    int A[N], B[N], s;
    cout << "Array A: \n";
    for (int i = 0; i < N; i++)
        cin >> A[i];
    cout << "Array B: \n";
    for (int i = 0; i < N; i++)
        cin >> B[i];
    for (int i = 0; i < N; i++) {
        s = A[i];
        A[i] = B[i];
        B[i] = s;
    }
    cout << "Array A = \n";
    for (int i = 0; i < N; i++)
        cout << A[i] << " ";
    cout << " \n";
    cout << "Array B =  \n";
    for (int i = 0; i < N; i++)
        cout << B[i] << " ";
    cout << " \n";
 
}
