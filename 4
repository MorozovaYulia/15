#include <iostream>
using namespace std;
int main()
{
    const int N = 10;
    int A[N];
    int max, min, Max, Min;
    for (int i = 0; i < N; i++)
        cin >> A[i];
    max = A[0];
    min = A[0];
    for (int i = 0; i < N; i++)
        if (max < A[i]) {
            max = A[i];
            Max = i;
        }
    for (int i = 0; i < N; i++)
        if (min > A[i]) {
            min = A[i];
            Min = i;
        }
    if (Max > Min)
        for (int i = Min + 1; i < Max; i++)
            A[i] = 0;
    else
        for (int i = Max + 1; i < Min; i++)
            A[i] = 0;
    for (int i = 0; i < N; i++)
        cout << A[i] << " ";
}
