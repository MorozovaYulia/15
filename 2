#include <iostream>
using namespace std;
int main()
{
    const int N = 10;
    int A[N];
    float B[N], a = 0;
    int j = 0; 
    
    for (int i = 0; i < N; i++)
        cin >> A[i];
    for (int i = 0; i < N; i++) {
        for (j; j <= i; j++)
            a += A[j];
        a /= j;
        j = 0;
        B[i] = a;
        a = 0;
    }
    for (int i = 0; i < N; i++)
        cout << B[i] << " ";
}
